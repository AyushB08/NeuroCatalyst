{
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static"
      },
      {
        "src": "static/**",
        "use": "@vercel/static"
      },
      {
        "src": "node_modules/three/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/favicon.ico",
        "dest": "/public/favicon.ico"
      },
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/node_modules/three/(.*)",
        "dest": "/node_modules/three/$1"
      }
    ]
  }
  